{
  "name": "snegprog/todo-list",
  "description": "todo-list",
  "type": "project",
  "authors": [
    {
      "name": "Negrey Sergey",
      "email": "snegprog@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4",
    "ext-yaml": "*",
    "doctrine/orm": "^2.8",
    "symfony/yaml": "^5.2",
    "bitexpert/disco": "^0.10.0",
    "league/route": "^5.0",
    "laminas/laminas-diactoros": "^2.6",
    "narrowspark/http-emitter": "^1.0",
    "monolog/monolog": "^2.2"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "*",
    "squizlabs/php_codesniffer": "*",
    "friendsofphp/php-cs-fixer": "*",
    "phpmd/phpmd" : "@stable",
    "sebastian/phpcpd": "*",
    "phpunit/phpunit": "*",
    "symfony/maker-bundle": "^1.31"
  }
}
